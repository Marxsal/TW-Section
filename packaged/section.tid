xp<nonWhitespace>]\">\n<$set name=seTemplate filter=\"[<currentSection>trim:prefix[]prefix[!]]\" \n      value=\"$:/plugins/kookma/section/templates/disclosure\" \n      emptyValue=\"$:/plugins/kookma/section/templates/foreword\">\n  <$transclude tiddler=<<seTemplate>> mode=inline/>\n</$set>\n</$list>\n</$vars>\n\\end"},"$:/plugins/kookma/section/macros/utility":{"title":"$:/plugins/kookma/section/macros/utility","created":"20211001132448596","modified":"20211006191151961","tags":"$:/tags/Section/Macro","type":"text/vnd.tiddlywiki","text":"\\define se-config-visibility-toolbar() $:/config/se/config-visibility-toolbar\n\\define se-editor()  $:/config/se/config-editor-type\n\n\\define stateTid() $:/state/se/$(source)$\n\\define editTid()  $:/content/se/$(source)$/$(counter)$ "},"$:/plugins/kookma/section/readme":{"title":"$:/plugins/kookma/section/readme","created":"20211001164011533","modified":"20211003153948985","tags":"","type":"text/vnd.tiddlywiki","text":"; Section Editor plugin\n: Edit, create, fold and manage a length, big tiddler in place by sections generated from html headings (h1, h2, ...)\n\n; Code and demo\nFor learning plugin features, syntax, tutorial and examples see the plugin demo and code pages\n\n* Demo: https://kookma.github.io/TW-Section/\n* Code: https://github.com/kookma/TW-Section\n"},"$:/plugins/kookma/section/settings":{"title":"$:/plugins/kookma/section/settings","created":"20211001164055844","modified":"20211007210349038","tags":"","type":"text/vnd.tiddlywiki","text":"\n{{$:/plugins/kookma/section/configs-ui}}\n---\n{{$:/plugins/kookma/section/ui/settings}}\n"},"$:/plugins/kookma/section/styles/conditional":{"title":"$:/plugins/kookma/section/styles/conditional","created":"20211001141824507","modified":"20211005152439431","tags":"$:/tags/Stylesheet","type":"text/vnd.tiddlywiki","text":"\\define assign-css()\n\\rules only\n[data-tiddler-title=\"$(currentTiddler)$\"] .tc-tiddler-body { display:none; }\n\\end\n<$list filter=\"[all[tiddlers]has:field[se-type]]\">\n<<assign-css>><br>\n</$list>\n\n\n<$list filter=\"[all[tiddlers]has:field[htmltag]]\">\n<<assign-css>><br>\n</$list>"},"$:/plugins/kookma/section/styles/details/02":{"title":"$:/plugins/kookma/section/styles/details/02","caption":"02 style","created":"20211006184619759","modified":"20211007203405198","tags":"","type":"text/vnd.tiddlywiki","text":".se-section details summary {\n  list-style: none;\n\tpadding-left:5px; /*me*/\n\n}\n.se-section details summary::-webkit-details-marker {\n  display: none;\n}\n.se-section details {\n  user-select: none;\n}\n\n.se-section details[open] summary svg {\n  transform: rotate(-90deg);\n}\n\n.se-section details summary svg {\n  transition: all 0.3s;\n}\n.se-section details summary svg{ /*me*/\nwidth: 1.2em;\nheight: 1.2em;\nvertical-align: middle;\nfloat:right;\n}\n\n.se-section details summary:focus {\n  outline: none;\n}\n\n.se-section details summary {\n  cursor: pointer;\n\tfont-weight: 900;\n\tfont-size:1.2em;\t\n\tpadding:15px;\n\tborder-radius:3px;\n}\n\n.se-section details summary:hover {\n  background: <<colour page-background>>;\n}\n\n/* by Mohammad */\n.pw{\n    opacity: 0;\n    transition: opacity 400ms;\n}\n.wrapper:hover .pw{ opacity: 1; }"},"$:/plugins/kookma/section/styles/details/boxed":{"title":"$:/plugins/kookma/section/styles/details/boxed","text":"/* Ref: https://developer.mozilla.org/ */\n.se-section details {\n    border: 1px solid #aaa;\n    border-radius: 4px;\n    padding: .5em .5em 0;\n}\n\n.se-section details summary {\n    font-weight: bold;\n    margin: -.5em -.5em 0;\n    padding: .5em;\n}\n\n.se-section details[open] {\n    padding: .5em;\n}\n\n.se-section details[open] summary {\n    border-bottom: 1px solid #aaa;\n    margin-bottom: .5em;\n}\n","caption":"Simple boxed style","created":"20211007093226311","modified":"20211007201614214","tags":"","type":"text/css"},"$:/plugins/kookma/section/styles/details/divided-sections":{"title":"$:/plugins/kookma/section/styles/details/divided-sections","text":".se-section details[open] summary ~ * {\n\tanimation: open 0.3s ease-in-out;\n}\n\n@keyframes open {\n\t0%   {opacity: 0;}\n\t100% {opacity: 1;}\n}\n.se-section details summary::-webkit-details-marker {\n\tdisplay: none;\n}\n\n.se-section details summary {\n\twidth: 100%;\n\tpadding: 0.5rem 0;\n\tborder-top: 1px solid #ccc;\n\tposition: relative;\n\tcursor: pointer;\n\tfont-size: 1.25rem;\n\tfont-weight: 300;\n\tlist-style: none;\n}\n\n.se-section details summary:after {\n\tcontent: \"+\";\n\t/*\tcolor: black;*/\n\tposition: absolute;\n\tfont-size: 1.75rem;\n\tline-height: 0;\n\tmargin-top: 0.75rem;\n\tright: 0;\n\tfont-weight: 200;\n\ttransform-origin: center;\n\ttransition: 200ms linear;\n}\n.se-section details[open] summary:after {\n\ttransform: rotate(45deg);\n\tfont-size: 2rem;\n}\n.se-section details summary {\n\toutline: 0;\n}\n/*\n.se-section details p {\nfont-size: 0.95rem;\nmargin: 0 0 1rem;\npadding-top: 1rem;\n}\n*/","caption":"Simple divided sections","created":"20211005202253877","modified":"20211007203257849","tags":"","type":"text/css"},"$:/plugins/kookma/section/styles/details/dt":{"title":"$:/plugins/kookma/section/styles/details/dt","text":".se-section details summary {list-style: none}\n.se-section details summary::-webkit-details-marker {display: none; }\n.se-section details summary{padding-left:0;}\n.se-section details[open] summary svg { transform: rotate(-90deg);}\n.se-section details summary svg {transition: all 0.3s;}\n.se-section details[open] summary{border-bottom:1px solid #ccc}\n.se-section details  svg {\n\tvertical-align:top;\n\theight: 1.5em;\n\twidth: 1.5em;\n}\n\n.se-section details div{margin:0.2rem}\n","caption":"dt style","created":"20211006071519409","modified":"20211007203359383","tags":"","type":"text/css"},"$:/plugins/kookma/section/styles/details/faq":{"title":"$:/plugins/kookma/section/styles/details/faq","text":".se-section :focus {\n\toutline: none;\n}\n\ndiv.se-section  + div.se-section  {\n\tclear: both;\n}\n\n.se-section div.seperator {\n\twidth: 7.5em;\n\theight: 0.07em;\n\tmargin-left: -1.8em;\n}\n\ndiv.se-section {\n\tborder-bottom: 0.07em solid #ededed;\n\tpadding: 1em 0em;\n} \n\ndiv.se-section:last-child {\n\tborder: initial;\n}\n\n.se-section details > summary {\n\tlist-style: none;\n}\n\n.se-section summary::-webkit-details-marker {\n\tdisplay: none;\n}\n\n.se-section details summary h1,\n.se-section details summary h2,\n.se-section details summary h3,\n.se-section details summary h4,\n.se-section details summary h5\n{\n\tfont-size: 1.4em;\n\tfont-weight: bold;\n\tcursor: pointer;\n\t-webkit-touch-callout: none;\n\t-webkit-user-select: none;\n\t-khtml-user-select: none;\n\t-moz-user-select: none;\n\t-ms-user-select: none; \n\tuser-select: none;\n\ttransition: all 0.3s ease;\n}\n\n.se-section details[open] .se-sectionbody {\n\tborder-left: 2px solid red;\n\tmargin-left: 40px;\n\tpadding-left: 25px;\n\topacity: 100;\n\ttransition: all 3s ease;\n}\n\n.se-section details[open] summary:after {\n\tcontent: \"-\";\n\tfont-size: 3.2em;\n\tmargin: -0.5em 0.14em 0 0;\n\tfont-weight: 300;\n}\n\n.se-section summary:after {\n\tbackground: transparent;\n\tborder-radius: 0.3em;\n\tcontent: \"+\";\n\tfloat: left;\n\tfont-size: 1.8em;\n\tfont-weight: 700;\n\tmargin: -0.3em 0.25em 0 0;\n\tpadding: 0;\n\ttext-align: center;\n\twidth: 25px;\n}\n\n/* color of summary and other elements*/\n.se-section div.seperator, \n.se-section summary:hover,\n.se-section details[open] summary,\n.se-section summary:after {\n\tcolor:#e31b1b;;    /* #e31b1b; */\n}\n\n\n/* animation is disabled\n.se-section details[open] .se-sectionbody {\nanimation: sweep .5s ease-in-out;\n}\n\n@keyframes sweep {\n0%    {opacity: 0; margin-left: -10px}\n100%  {opacity: 1; margin-left: 40px}\n}\n*/","caption":"Faq style","created":"20211007163350283","modified":"20211007205715261","tags":"","type":"text/css"},"$:/plugins/kookma/section/styles/disclosure":{"title":"$:/plugins/kookma/section/styles/disclosure","created":"20211007112837487","modified":"20211007202617657","tags":"$:/tags/Stylesheet","type":"text/vnd.tiddlywiki","text":"<$transclude tiddler={{$:/plugins/kookma/section/configs}} field=text mode=block/>"},"$:/plugins/kookma/section/styles/main":{"title":"$:/plugins/kookma/section/styles/main","text":".se-section{ /*wrapp a single section*/\n\tmargin-bottom:5px;\n} \n.se-btn{\n\topacity:0;  /* autohide the edit button on section */\n\tfloat:right;\n\tmargin-right:6px;\n}\n.se-section:hover .se-btn{\n\topacity:1;\n}\n\n.se-simple-textarea{\n\tmargin-top: 6px; \n\tpadding: 6px;\n\n\twidth:100%;\n\tmin-height: 40px;\n\tresize: none; /* vertical user can resize vertically, but width is fixed */\n}\n\n.se-sectionbody {\n margin-top:2.5rem; /* set distance to edit/done/cancel buttons*/\n}","created":"20211001135556291","modified":"20211007151330863","tags":"$:/tags/Stylesheet","type":"text/css"},"$:/plugins/kookma/section/templates/disclosure":{"title":"$:/plugins/kookma/section/templates/disclosure","created":"20211007064243389","hide-body":"yes","modified":"20211007151237200","tags":"","type":"text/vnd.tiddlywiki","text":"<$vars linebreak=\"\"\"\n\"\"\"><div class=\"se-section\"><details open><$list filter=\"[<currentSection>splitregexp[\\n]first[]]\" variable=sectionHeader>\n<summary><$transclude tiddler=\"$:/plugins/kookma/section/templates/section-header\" mode=inline/></summary>\n</$list><$list filter=\"[<currentSection>splitregexp[\\n]rest[]join<linebreak>]  ~[[&nbsp;]]\" variable=sectionBody>\n<$transclude tiddler=\"$:/plugins/kookma/section/templates/section-body\" mode=block/>\n</$list></details></div></$vars>\n\n<!-- important note\nfilter=\"[<currentSection>splitregexp[\\n]rest[]join<linebreak>]  ~[[&nbsp;]]\"\nThe extra  ~[[&nbsp;]] address the issue: a single header line at the bottom doesn’t create a section icon.\n-->"},"$:/plugins/kookma/section/templates/edit":{"title":"$:/plugins/kookma/section/templates/edit","created":"20211005104151321","hide-body":"yes","modified":"20211005141628239","tags":"","type":"text/vnd.tiddlywiki","text":"<$keyboard actions=<<doneActions>> key=\"ctrl+enter\">\n<$keyboard actions=<<cancelActions>> key=\"escape\">\n<$reveal type=\"nomatch\" state=<<se-editor>> text=\"main-editor\" default=\"simple-textarea\">\n<$edit-text tiddler=<<editTid>> field=\"text\" tag=textarea class=\"se-simple-textarea\"/>\n</$reveal>\n<$reveal type=\"match\" state=<<se-editor>> text=\"main-editor\" default=\"simple-textarea\">\n<$list filter=\"[<editTid>]\" template=\"$:/plugins/kookma/section/ui/editor\"/>\n</$reveal>\n</$keyboard>\n</$keyboard>"},"$:/plugins/kookma/section/templates/foreword":{"title":"$:/plugins/kookma/section/templates/foreword","created":"20211007055453257","hide-body":"yes","modified":"20211007063740183","tags":"","type":"text/vnd.tiddlywiki","text":"<!-- display foreword without sectionizing -->\n<$transclude tiddler=<<currentSection>> field=title mode=\"block\"/>"},"$:/plugins/kookma/section/templates/section-body":{"title":"$:/plugins/kookma/section/templates/section-body","created":"20211005104853941","hide-body":"yes","modified":"20211007173509653","tags":"","type":"text/vnd.tiddlywiki","text":"\\define editActions()\n<!-- action when edit button is clicked -->\n<$action-setfield $tiddler=<<stateTid>> $index=<<counter>> $value=\"edit\" />\n<$action-setfield $tiddler=<<editTid>> $field=text $value=<<currentSection>> />\n\\end\n\n\\define doneActions()\n<!-- action when done button is clicked -->\n<$action-setfield $tiddler=<<stateTid>> $index=<<counter>> $value=\"view\" />\n<$vars for=<<currentSection>> with={{{[<editTid>get[text]]}}}>\n<$action-setfield $tiddler=<<source>> $field=text $value={{{[<source>get[text]search-replace<for>,<with>]}}} />\n<$action-deletetiddler $tiddler=<<editTid>> />\n</$vars>\n\\end\n\n\\define cancelActions()\n<!-- action when cancel button is clicked -->\n<$action-setfield $tiddler=<<stateTid>> $index=<<counter>> $value=\"view\" />\n<$action-deletetiddler $tiddler=<<editTid>> />\n\\end\n\n<!-- Toggle and action buttons -->\n<$reveal type=\"nomatch\" stateTitle=<<stateTid>> stateIndex=<<counter>> text=\"edit\">\n    <$button actions=<<editActions>> class=\"tc-btn-invisible se-btn\">{{$:/core/images/edit-button}}</$button>\n</$reveal>\n<$reveal type=\"match\" stateTitle=<<stateTid>> stateIndex=<<counter>>  text=\"edit\">\n    <$button actions=<<doneActions>>   class=\"tc-btn-invisible se-btn\">{{$:/core/images/done-button}}  </$button>\n    <$button actions=<<cancelActions>> class=\"tc-btn-invisible se-btn\">{{$:/core/images/cancel-button}}</$button>\n</$reveal>\n\n<!-- Display section in edit or view mode -->\n<$reveal stateTitle=<<stateTid>> stateIndex=<<counter>> type=\"nomatch\" text=\"edit\" tag=div class=\"se-sectionbody\">\n<$transclude tiddler=\"$:/plugins/kookma/section/templates/view\" mode=block/>\n</$reveal>\n<$reveal stateTitle=<<stateTid>> stateIndex=<<counter>> type=\"match\"   text=\"edit\" tag=div class=\"se-sectionbody\">\n<$transclude tiddler=\"$:/plugins/kookma/section/templates/edit\" mode=block/>\n</$reveal>"},"$:/plugins/kookma/section/templates/section-header":{"title":"$:/plugins/kookma/section/templates/section-header","created":"20211006062426339","hide-body":"yes","modified":"20211007085623464","tags":"","type":"text/vnd.tiddlywiki","text":"\\whitespace trim\n\\define header() <$(hn)$><$text text={{{ [<sectionHeader>trim:prefix[!]] }}} /></$(hn)$>\n\n<$set name=hn filter=\"[<sectionHeader>search-replace::regexp[(^!*).*],[$1]length[]addprefix[h]]\" ><<header>></$set>"},"$:/plugins/kookma/section/templates/view":{"title":"$:/plugins/kookma/section/templates/view","created":"20211005104214815","modified":"20211007091540180","tags":"","type":"text/vnd.tiddlywiki","text":"<$transclude tiddler=<<sectionBody>> field=title mode=\"block\"/>"},"$:/plugins/kookma/section/ui/editor":{"title":"$:/plugins/kookma/section/ui/editor","created":"20210930210542075","hide-body":"yes","modified":"20211007130649687","tags":"","type":"text/vnd.tiddlywiki","text":"\\import $:/plugins/kookma/section/macros/settings\n<$edit\n\n  field=\"text\"\n  class=\"tc-edit-texteditor tc-edit-texteditor-body\"\n  placeholder={{$:/language/EditTemplate/Body/Placeholder}}\n  tabindex={{$:/config/EditTabIndex}}\n  focus={{{ [{$:/config/AutoFocus}match[text]then[true]] ~[[false]] }}}\n  cancelPopups=\"yes\"\n  fileDrop={{{ [{$:/config/DragAndDrop/Enable}match[no]] :else[subfilter{$:/config/Editor/EnableImportFilter}then[yes]else[no]] }}}\n\n><$set\n\n  name=\"targetTiddler\"\n  value=<<currentTiddler>>\n\n><$reveal\n  type=\"match\"\n  state=<<se-config-visibility-toolbar>>\n  text=\"yes\"\n><$list\n\n  filter=\"[all[shadows+tiddlers]tag[$:/tags/EditorToolbar]!has[draft.of]] -[[$:/core/ui/EditorToolbar/preview]] -[[$:/core/ui/EditorToolbar/preview-type]]\"\n\n><$reveal\n\n  type=\"nomatch\"\n  state=<<config-visibility-title>>\n  text=\"hide\"\n  class=\"tc-text-editor-toolbar-item-wrapper\"\n\n><$transclude\n\n  tiddler=\"$:/core/ui/EditTemplate/body/toolbar/button\"\n  mode=\"inline\"\n\n/></$reveal></$list><$list\n\n  filter=\"[all[shadows+tiddlers]tag[$:/tags/EditorTools]!has[draft.of]]\"\n\n><$list\n\tfilter={{!!condition}}\n\tvariable=\"list-condition\"\n><$transclude/>\n</$list></$list></$reveal></$set></$edit>\n"},"$:/plugins/kookma/section/ui/settings":{"title":"$:/plugins/kookma/section/ui/settings","created":"20210930213246035","modified":"20211006081210387","tags":"","type":"text/vnd.tiddlywiki","text":"\\import $:/plugins/kookma/section/macros/utility\n\n; Set editor (main editor/ simple text area)\n:<$checkbox tiddler=<<se-editor>> field=\"text\" checked=\"main-editor\" unchecked=\"simple-textarea\" default=\"simple-textarea\"> <$link to=<<se-editor>> ></$link> </$checkbox>\n\n; Turn on/off editor toolbar (for main editor)\n:<$checkbox tiddler=<<se-config-visibility-toolbar>> field=\"text\" checked=\"yes\" unchecked=\"no\" default=\"yes\"> <$link to=<<se-config-visibility-toolbar>> ></$link> </$checkbox>\n\n<<details label:\"More Info\" src:\"\"\"\n\n* Main editor\n** The same editor isused for tiddler editing like ~CodeMirror\n** Simple text area is the basic editor\n\n** One and off toolbar\n* When the main editor is used, the editor tollbar can be on/off\n\n\"\"\">>"},"$:/plugins/kookma/section/viewtemplates/sections":{"title":"$:/plugins/kookma/section/viewtemplates/sections","created":"20211001140102474","modified":"20211007204701716","tags":"$:/tags/ViewTemplate","type":"text/vnd.tiddlywiki","text":"\\define pattern() \\n(?=!{1,$(hn)$}[^!])\n\n<$list filter=\"[all[current]has:field[se-type]]\">\n\n<$reveal tag=\"div\" class=\"xtc-tiddler-body\" type=\"nomatch\" stateTitle=<<folded-state>> text=\"hide\" retain=\"yes\" animate=\"yes\">\n\n<$list filter=\"[all[current]!has[plugin-type]!field:hide-body[yes]]\" variable=NULL>\n\n<$vars hn={{{ [[$:/plugins/kookma/section/configs-hn]get[text]else[2]] }}} > <!-- default number of headings 2 -->\n<$macrocall $name=section-editor source=<<currentTiddler>> pattern=<<pattern>> />\n</$vars>\n\n</$list>\n\n</$reveal>\n<!-- generate sections only by first two levels -->\n\n\n\n"}}}